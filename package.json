{
  "name": "FCheatlauncher",
  "version": "1.1.8",
  "description": "FCheatLauncher By AutoEZ_TW",
  "main": "main.js",
  "icon": "assets/images/app-logo-nobg.ico",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "decompress": "^4.2.1",
    "electron": "^31.6.0",
    "electron-builder": "^24.13.3",
    "electron-reload": "^2.0.0-alpha.1",
    "rollup": "^4.22.4",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "antd": "^5.20.0",
    "axios": "^1.7.7",
    "bootstrap-icons": "^1.11.3",
    "decompress": "^4.2.1",
    "dialog": "^0.3.1",
    "discord-rich-presence": "^0.0.8",
    "discord-rpc": "^4.0.1",
    "dotenv": "^16.4.5",
    "electron-log": "^5.2.0",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9",
    "fs-extra": "^11.2.0",
    "node-unrar-js": "^2.0.2",
    "python": "^0.0.4",
    "react": "^19.0.0",
    "sudo-prompt": "^9.2.1",
    "unrar": "^0.2.0",
    "unzipper": "^0.12.3",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "winston": "^3.17.0"
  },
  "build": {
    "appId": "com.fcheat.launcher",
    "productName": "FCheatLauncher",
    "directories": {
      "output": "launcherOutput"
    },
    "files": [
      "launcher/**",
      "assets/**/*",
      "node_modules/**/*",
      "main.js",
      "renderer.js",
      "preload.js",
      "launcher.html",
      "menu.html",
      "menu-preload.js",
      "services/**",
      "config/**"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/images/app-logo-nobg.ico",
      "requestedExecutionLevel": "requireAdministrator",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "nsis": {
      "oneClick": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "${productName}",
      "installerIcon": "assets/images/app-logo-nobg.ico",
      "uninstallerIcon": "assets/images/app-logo-nobg.ico"
    },
    "asar": true
  }
}
